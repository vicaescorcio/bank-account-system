<p id="notice"><%= notice %></p>


<h4>Contas</h4>
    <table class="centered responsive-table highlight">
        <thead>
          <tr>
              <th>Ag</th>
              <th>Cc</th>
              <th>Limite</th>
              <th colspan="3"></th>
          </tr>
        </thead>
        <tbody id ='accounts'>
         <% @accounts.each do |account|%>
          <tr>
            <td><%=account.agency.ag%></td>
            <td><%=account.cc%></td>
            <td><%=account.limit%></td>
            <td><%= link_to 'Edit', edit_account_path(account) %></td>
            <td><%= link_to 'Destroy', account, method: :delete, data: { confirm: 'Are you sure?' } %></td>
         
          </tr>
          <%end%>
        </tbody>
      </table>
          
<%= link_to  new_account_path,class:'btn-floating btn-large waves-effect waves-light red', remote: true do %>
<i class="material-icons">add</i>  
<%end%>